# Docling Converter Settings

# Output dir for single PDF conversions (blank = ./outputs)
SINGLE_PDF_OUTPUT_DIR=

WEBP_ENABLED=true
# Quality 0-100 (65 = good balance)
WEBP_QUALITY=65
# Compression effort 0-6 (6 = best/slowest)
WEBP_METHOD=6
# Max dimensions (0 = no limit)
WEBP_MAX_WIDTH=1920
WEBP_MAX_HEIGHT=1080
